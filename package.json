{
  "name": "mmd-react-ssr",
  "version": "0.1.0",
  "main": "build/server.js",
  "license": "MIT",
  "scripts": {
    "start": "yarn clean && cross-env NODE_ENV=development webpack-dev-server --watch --config webpacks/configGenerator.js",
    "start-dev-server": "cross-env NODE_ENV=development nodemon .build/server.js",
    "build": "yarn clean && cross-env NODE_ENV=production webpack --config webpacks/configGenerator.js",
    "clean": "rimraf -rf build",
    "start-prod-server": "nodemon build/server.js",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "lint:js": "eslint --fix src/**/*.{js,jsx}",
    "lint:styles": "stylelint --fix src/**/*.{css,scss,less,sass}"
  },
  "dependencies": {
    "@loadable/component": "^5.12.0",
    "@loadable/server": "^5.12.0",
    "axios": "^0.19.2",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "serialize-javascript": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.1",
    "@hot-loader/react-dom": "^16.13.0",
    "@loadable/babel-plugin": "^5.12.0",
    "@loadable/webpack-plugin": "^5.12.0",
    "@testing-library/react": "^10.0.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.1.0",
    "chokidar": "^3.3.1",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.2",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "file-loader": "^6.0.0",
    "husky": "^4.2.3",
    "internal-ip": "^6.0.0",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.8",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.2",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-flexbugs-fixes": "^4.2.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.19.1",
    "prettier-eslint": "^9.0.1",
    "react-hot-loader": "^4.12.20",
    "react-test-renderer": "^16.13.1",
    "resolve-url-loader": "^3.1.1",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "shelljs": "^0.8.3",
    "style-loader": "^1.1.3",
    "stylelint": "^13.2.1",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-scss": "^3.16.0",
    "terser-webpack-plugin": "^2.3.5",
    "url-loader": "^4.0.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2",
    "webpackbar": "^4.0.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "yarn lint:js"
    ],
    "src/**/*.{css,scss,less,sass}": [
      "yarn lint:styles"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      "last 10 versions",
      "> 1%",
      "ie 10",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "nodemonConfig": {
    "watch": [
      "build/server.js"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/node_modules/babel-jest"
    }
  }
}
