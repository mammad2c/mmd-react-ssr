{
  "name": "mmd-react-ssr",
  "version": "0.1.0",
  "main": "build/server.js",
  "license": "MIT",
  "scripts": {
    "start": "yarn clean && yarn dev:client && dev:server",
    "dev:client": "NODE_ENV=development NODE_TARGET=web webpack-dev-server --watch --config webpacks/webpack.config.client.js",
    "dev:server": "NODE_ENV=development NODE_TARGET=node webpack --watch --config webpacks/webpack.config.server.js",
    "start-dev-server": "NODE_ENV=development nodemon .build/server.js",
    "build": "yarn clean && yarn prod:client && yarn prod:server",
    "prod:client": "NODE_ENV=production NODE_TARGET=web webpack --config webpacks/webpack.config.client.js --production",
    "prod:server": "NODE_ENV=production NODE_TARGET=node webpack --config webpacks/webpack.config.server.js --production",
    "clean": "rimraf -rf build",
    "start-prod-server": "nodemon build/server.js",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-preset-env": "^6.7.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-hot-loader": "^4.12.18",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "serialize-javascript": "^2.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "@hot-loader/react-dom": "^16.11.0",
    "@testing-library/react": "^9.3.2",
    "assets-webpack-plugin": "^3.9.10",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "eslint": "^6.7.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "file-loader": "^4.3.0",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.3",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.13.0",
    "nodemon": "^2.0.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "prettier-eslint": "^9.0.1",
    "react-test-renderer": "^16.12.0",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "shelljs": "^0.8.3",
    "style-loader": "^1.0.0",
    "stylelint": "^12.0.0",
    "stylelint-config-recommended-scss": "^4.1.0",
    "stylelint-scss": "^3.13.0",
    "terser-webpack-plugin": "^2.2.1",
    "url-loader": "^2.3.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2",
    "webpackbar": "^4.0.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "src/**/*.{css,scss,less,sass}": [
      "stylelint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      "last 10 versions",
      "> 1%",
      "ie 10",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "nodemonConfig": {
    "watch": [
      "build"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/node_modules/babel-jest"
    }
  }
}
