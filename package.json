{
  "name": "mmd-react-ssr",
  "version": "0.1.0",
  "main": "build/server.js",
  "license": "MIT",
  "scripts": {
    "start": "yarn clean && cross-env NODE_ENV=development webpack-dev-server --watch --config webpacks/configGenerator.js",
    "start-dev-server": "cross-env NODE_ENV=development nodemon .build/server.js",
    "build": "yarn clean && cross-env NODE_ENV=production webpack --config webpacks/configGenerator.js",
    "clean": "rimraf -rf build",
    "start-prod-server": "nodemon build/server.js",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "lint:js": "eslint --fix src/**/*.{js,jsx}",
    "lint:styles": "stylelint --fix src/**/*.{css,scss,less,sass}"
  },
  "dependencies": {
    "@loadable/component": "5.13.1",
    "@loadable/server": "5.13.1",
    "axios": "0.20.0",
    "cors": "2.8.5",
    "express": "4.17.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.1.0",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "serialize-javascript": "4.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.11.6",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/plugin-transform-runtime": "7.11.5",
    "@babel/preset-env": "7.11.5",
    "@babel/preset-react": "7.10.4",
    "@loadable/babel-plugin": "5.13.0",
    "@loadable/webpack-plugin": "5.13.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.2",
    "@testing-library/react": "11.0.2",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.3.0",
    "babel-loader": "8.1.0",
    "chokidar": "3.4.2",
    "copy-webpack-plugin": "6.1.0",
    "cross-env": "7.0.2",
    "css-loader": "4.2.2",
    "eslint": "7.8.1",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.6",
    "file-loader": "6.1.0",
    "husky": "4.2.5",
    "internal-ip": "6.1.0",
    "jest": "26.4.2",
    "lint-staged": "10.3.0",
    "mini-css-extract-plugin": "0.11.0",
    "node-sass": "4.14.1",
    "nodemon": "2.0.4",
    "optimize-css-assets-webpack-plugin": "5.0.4",
    "postcss-flexbugs-fixes": "4.2.1",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.7.0",
    "prettier": "2.1.1",
    "prettier-eslint": "11.0.0",
    "react-refresh": "^0.8.3",
    "react-test-renderer": "16.13.1",
    "resolve-url-loader": "3.1.1",
    "rimraf": "3.0.2",
    "sass-loader": "10.0.2",
    "shelljs": "0.8.4",
    "style-loader": "1.2.1",
    "stylelint": "13.7.0",
    "stylelint-config-recommended-scss": "4.2.0",
    "stylelint-scss": "3.18.0",
    "terser-webpack-plugin": "4.1.0",
    "url-loader": "4.1.0",
    "webpack": "4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "3.11.0",
    "webpack-node-externals": "2.5.2",
    "webpackbar": "4.0.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "yarn lint:js"
    ],
    "src/**/*.{css,scss,less,sass}": [
      "yarn lint:styles"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      "last 10 versions",
      "> 1%",
      "ie 10",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      "build/*"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/node_modules/babel-jest"
    }
  }
}
