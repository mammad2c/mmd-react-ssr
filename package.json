{
  "name": "mmd-react-ssr",
  "version": "0.1.0",
  "main": "build/server.js",
  "license": "MIT",
  "scripts": {
    "start": "yarn clean && cross-env NODE_ENV=development webpack-dev-server --watch --config webpacks/configGenerator.js",
    "start-dev-server": "cross-env NODE_ENV=development nodemon .build/server.js",
    "build": "yarn clean && cross-env NODE_ENV=production webpack --config webpacks/configGenerator.js",
    "clean": "rimraf -rf build",
    "start-prod-server": "nodemon build/server.js",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "lint:js": "eslint --fix src/**/*.{js,jsx}",
    "lint:styles": "stylelint --fix src/**/*.{css,scss,less,sass}"
  },
  "dependencies": {
    "@loadable/component": "^5.12.0",
    "@loadable/server": "^5.12.0",
    "axios": "^0.19.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "serialize-javascript": "^2.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@hot-loader/react-dom": "^16.11.0",
    "@loadable/babel-plugin": "^5.12.0",
    "@loadable/webpack-plugin": "^5.12.0",
    "@testing-library/react": "^9.4.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "chokidar": "^3.3.1",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.0",
    "file-loader": "^5.0.2",
    "husky": "^4.0.10",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.2",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.19.1",
    "prettier-eslint": "^9.0.1",
    "react-hot-loader": "^4.12.18",
    "react-test-renderer": "^16.12.0",
    "resolve-url-loader": "^3.1.1",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.2",
    "shelljs": "^0.8.3",
    "style-loader": "^1.1.3",
    "stylelint": "^13.0.0",
    "stylelint-config-recommended-scss": "^4.1.0",
    "stylelint-scss": "^3.13.0",
    "terser-webpack-plugin": "^2.3.2",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2",
    "webpackbar": "^4.0.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "yarn lint:js",
      "git add"
    ],
    "src/**/*.{css,scss,less,sass}": [
      "yarn lint:styles",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      "last 10 versions",
      "> 1%",
      "ie 10",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "nodemonConfig": {
    "delay": "1000",
    "watch": [
      "build"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/node_modules/babel-jest"
    }
  }
}
