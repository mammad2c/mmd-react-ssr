{
  "name": "mmd-react-ssr",
  "version": "0.1.0",
  "main": "build/server.js",
  "license": "MIT",
  "scripts": {
    "start": "node ./scripts/run.js",
    "build": "node ./scripts/build.js",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "lint:js": "eslint --fix src/**/*.{js,jsx}",
    "lint:styles": "stylelint --fix src/**/*.{css,scss,less,sass}",
    "format": "prettier --write ./**/*.{js,jsx,css,scss,less,sass,json,.md}",
    "postinstall": "husky install",
    "lint-staged": "lint-staged"
  },
  "engines": {
    "npm": "please-use-yarn",
    "yarn": ">= 1.22.x",
    "node": ">= 12.x.x"
  },
  "dependencies": {
    "@loadable/component": "5.15.0",
    "@loadable/server": "5.15.0",
    "axios": "0.21.1",
    "cors": "2.8.5",
    "express": "4.17.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "serialize-javascript": "5.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.14.3",
    "@babel/plugin-proposal-class-properties": "7.13.0",
    "@babel/plugin-transform-runtime": "7.14.3",
    "@babel/preset-env": "7.14.4",
    "@babel/preset-react": "7.13.13",
    "@hot-loader/react-dom": "17.0.1",
    "@loadable/babel-plugin": "5.13.2",
    "@loadable/webpack-plugin": "5.15.0",
    "@testing-library/react": "11.2.7",
    "@types/jest": "^26.0.23",
    "babel-eslint": "10.1.0",
    "babel-jest": "27.0.2",
    "babel-loader": "8.2.2",
    "chalk": "4.1.1",
    "copy-webpack-plugin": "9.0.0",
    "css-loader": "5.2.6",
    "eslint": "7.28.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.24.0",
    "file-loader": "6.2.0",
    "friendly-errors-webpack-plugin": "1.7.0",
    "husky": "6.0.0",
    "internal-ip": "6.2.0",
    "jest": "27.0.4",
    "lint-staged": "11.0.0",
    "mini-css-extract-plugin": "1.6.0",
    "optimize-css-assets-webpack-plugin": "6.0.0",
    "postcss": "^8.3.0",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-loader": "5.3.0",
    "postcss-preset-env": "6.7.0",
    "prettier": "2.3.1",
    "prettier-eslint": "12.0.0",
    "razzle-start-server-webpack-plugin": "^4.0.4",
    "react-hot-loader": "4.13.0",
    "react-test-renderer": "17.0.2",
    "resolve-url-loader": "4.0.0",
    "rimraf": "3.0.2",
    "sass": "^1.34.1",
    "sass-loader": "12.0.0",
    "style-loader": "2.0.0",
    "stylelint": "13.13.1",
    "stylelint-config-recommended-scss": "4.2.0",
    "stylelint-scss": "3.19.0",
    "terser-webpack-plugin": "5.1.3",
    "url-loader": "4.1.1",
    "webpack": "5.38.1",
    "webpack-dev-server": "3.11.2",
    "webpack-node-externals": "3.0.0",
    "webpackbar": "5.0.0-3"
  },
  "lint-staged": {
    "*.{js,jsx}": "eslint --fix",
    "*.{css,scss,less,sass}": "stylelint --fix",
    "*.{js,jsx,css,scss,less,sass,json,.md}": "prettier --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      "last 10 versions",
      "> 1%",
      "ie 10",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/node_modules/babel-jest"
    }
  }
}
